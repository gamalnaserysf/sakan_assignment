{
  "config": {
	"preferred-install": "dist",
	"optimize-autoloader": true,
	"allow-plugins": {
	  "composer/installers": true,
	  "oomphinc/composer-installers-extender": true
	}
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "extra": {
	"installer-types": [
	  "library",
	  "component"
	],
	"installer-paths": {
	  "../assets/composer-asset/{$vendor}/{$name}/": [
		"type:component"
	  ]
	}
  },
  "require": {
	"php": ">=7.3.0",
	"codeigniter/framework": "3.1.13",
	"chriskacerguis/codeigniter-restserver": "^3.0",
	"ext-json": "*"
  }
}
